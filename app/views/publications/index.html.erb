<%= render :partial => "assets/new_button", :locals => {:new_object_path => new_publication_path, :label => "Register a publication"} %>

<% if Seek::Config.faceted_browsing_enabled && Seek::Config.facet_enable_for_pages[controller_name]  %>
    <%= render :partial => "faceted_browsing/facets", :object => @publications %>
<% else %>
    <%= render :partial => "assets/resources_alphabetical_paginated", :object => @publications %>
<% end %>