
<div id="run-outputs">
  <% if run.outputs.size > 0 %>
    <h2>Outputs</h2>

    <%= link_to "Download All", run.results.url %>

    <% run.outputs.each do |output| %>
      <p>
        <b>Name:</b>
        <%= output.name %>
      </p>

      <p>
        <b>Value:</b>
        <%= TavernaPlayer.port_renderer.render(output) %>
      </p>

    <% end%>
  <% end %>
</div>
