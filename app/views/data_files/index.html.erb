<%= render :partial => "assets/new_button", :locals => {:new_object_path => new_data_file_path, :label => "Upload a #{t('data_file')}"} %>

<% if Seek::Config.faceted_browsing_enabled && Seek::Config.facet_enable_for_pages[controller_name]  %>
    <%= render :partial => "faceted_browsing/facets", :object => @data_files %>
<% else %>
    <%= render :partial => "assets/resources_alphabetical_paginated", :object => @data_files %>
<% end %>