<% if can_view_asset?(@data_file, params[:code]) -%>

  <div>
    <% unless @display_data_file.studied_factors.empty? -%>
      <ul>
        <%= render :partial=>"studied_factors/condition_or_factor_row_headings", :locals=>{:asset => 'data_file', :show_delete=>false} %>
        <%= render :partial=>"studied_factors/condition_or_factor_row",:collection=>@display_data_file.studied_factors, :locals=>{:asset => 'data_file', :show_delete=>false} %>
      </ul>
    <% else -%>
      <span class="none_text">No factors studied for this <%= t('data_file') %>.</span>
    <% end -%>
  </div>

  <div style="clear:both;">&nbsp;</div>

    <% if @data_file.can_edit? -%>
        <%= button_link_to('Edit factors studied (latest version)', 'edit', data_file_studied_factors_path(@data_file)) -%>
    <% end -%>

<% end -%>
