<% if logged_in? and current_user.person.member? %>
    <ul class="sectionIcons">
      <li><%= image_tag_for_key "new", new_specimen_path, "New #{(I18n.t 'biosamples.sample_parent_term').capitalize}", nil, "New #{(I18n.t 'biosamples.sample_parent_term').capitalize}" %></li>
    </ul>
<% end %>

<% if Seek::Config.faceted_browsing_enabled && Seek::Config.facet_enable_for_pages[controller_name]  %>
    <%= render :partial => "faceted_browsing/facets", :object => @specimens %>
<% else %>
    <%= render :partial => "assets/resources_alphabetical_paginated", :object => @specimens %>
<% end %>


