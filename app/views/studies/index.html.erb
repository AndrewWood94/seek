<% if logged_in? and current_user.person.member?%>
    <ul class="sectionIcons">
        <li><%= image_tag_for_key "new", new_study_path, "New #{t('study')}", nil, "New #{t('study')}" %></li>
    </ul>
<% end %>

<% if Seek::Config.faceted_browsing_enabled && Seek::Config.facet_enable_for_pages[controller_name]  %>
    <%= render :partial => "faceted_browsing/facets", :object => @studies %>
<% else %>
    <%= render :partial => "assets/resources_alphabetical_paginated", :object => @studies %>
<% end %>