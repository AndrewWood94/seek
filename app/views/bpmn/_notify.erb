<serviceTask id="<%= id %>" name="<%= name %>" flowable:type="mail">
<extensionElements>
<flowable:field name="to">
<flowable:string>
<![CDATA[ <%= assignee %> ]]>
</flowable:string>
</flowable:field>
<flowable:field name="from">
<flowable:string>
<![CDATA[ ibisbahub@inra.fr ]]>
</flowable:string>
</flowable:field>
<flowable:field name="subject">
<flowable:string>
<![CDATA[ <%= name %> is ready to start]]>
</flowable:string>
</flowable:field>
<flowable:field name="cc">
<flowable:string>
<![CDATA[ alan.r.williams@manchester.ac.uk ]]>
</flowable:string>
</flowable:field>
<flowable:field name="text">
<flowable:string>
<![CDATA[ This is sent from a flowable mail task ]]>
</flowable:string>
</flowable:field>
</extensionElements>
</serviceTask>
