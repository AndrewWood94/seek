{
  "$schema": "http://json-schema.org/draft-04/schema#", 
  "additionalProperties": false, 
  "definitions": {}, 
  "properties": {
    "data": {
      "additionalItems": false, 
      "items": {
        "additionalProperties": false, 
        "properties": {
          "id": {
            "type": "string"
          }, 
          "type": {
            "enum": [
              "publications"
            ], 
            "type": "string"
          }
        }, 
        "required": [
          "id", 
          "type"
        ], 
        "type": "object"
      }, 
      "minItems": 0, 
      "type": "array", 
      "uniqueItems": true
    }, 
    "jsonapi": {
      "additionalProperties": false, 
      "properties": {
        "version": {
          "enum": [
            "1.0"
          ], 
          "type": "string"
        }
      }, 
      "required": [
        "version"
      ], 
      "type": "object"
    }, 
    "meta": {
      "additionalProperties": false, 
      "properties": {
        "base_url": {
          "type": "string",
          "format" : "uri"
        }
      }, 
      "required": [
        "base_url"
      ], 
      "type": "object"
    }
  }, 
  "required": [
    "jsonapi", 
    "data", 
    "meta"
  ], 
  "type": "object"
}
