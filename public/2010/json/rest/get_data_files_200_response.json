{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"data": {
			"type": "object",
			"properties": {
				"id": {
					"$ref": "definitions.json#/definitions/nonEmptyString"
				},
				"type": {
					"$ref": "definitions.json#/definitions/dataFilesType"
				},
				"attributes": {
					"type": "object",
					"properties": {
						"tags": {
							"$ref": "definitions.json#/definitions/tagsType"
						},
						"title": {
							"$ref": "definitions.json#/definitions/nonEmptyString"
						},
						"description": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"$ref": "definitions.json#/definitions/nonEmptyString"
								}
							]
						},
						"version": {
							"type": "number"
						},
						"license": {
							"$ref": "definitions.json#/definitions/nonEmptyString"
						},
						"latest_version": {
							"type": "object",
							"properties": {
								"title": {
									"$ref": "definitions.json#/definitions/nonEmptyString"
								},
								"description": {
									"oneOf": [
										{
											"type": "null"
										},
										{
											"$ref": "definitions.json#/definitions/nonEmptyString"
										}
									]
								},
								"version": {
									"type": "number"
								},
								"revision_comments": {
									"oneOf": [
										{
											"type": "null"
										},
										{
											"$ref": "definitions.json#/definitions/nonEmptyString"
										}
									]
								},
								"license": {
									"$ref": "definitions.json#/definitions/nonEmptyString"
								},
								"uuid": {
									"$ref": "definitions.json#/definitions/nonEmptyString"
								},
								"created_at": {
									"$ref": "definitions.json#/definitions/dateTimeString"
								},
								"updated_at": {
									"$ref": "definitions.json#/definitions/dateTimeString"
								}
							},
							"required": [
								"title",
								"description",
								"version",
								"license",
								"uuid",
								"created_at",
								"updated_at"
							]
						},
						"content_blobs": {
							"type": "array",
							"minItems": 1,
							"uniqueItems": true,
							"items": {
								"properties": {
									"content_blob_id": {
										"$ref": "definitions.json#/definitions/nonEmptyString"
									},
									"original_filename": {
										"$ref": "definitions.json#/definitions/nonEmptyString"
									},
									"content_type": {
										"$ref": "definitions.json#/definitions/nonEmptyString"
									},
									"is_webpage": {
										"type": "boolean"
									},
									"external_link": {
										"type": "boolean"
									},
									"file_size": {
										"type": "number"
									},
									"url": {
										"oneOf": [
											{
												"type": "null"
											},
											{
												"$ref": "definitions.json#/definitions/uriString"
											}
										]
									},
									"uuid": {
										"$ref": "definitions.json#/definitions/nonEmptyString"
									}
								},
								"required": [
									"content_blob_id",
									"original_filename",
									"content_type",
									"is_webpage",
									"external_link",
									"file_size",
									"uuid"
								]
							}
						},
						"current_revision_comments": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"$ref": "definitions.json#/definitions/nonEmptyString"
								}
							]
						}
					},
					"required": [
						"title",
						"version",
						"license",
						"latest_version",
						"content_blobs"
					]
				},
				"relationships": {
					"type": "object",
					"properties": {
						"creators": {
							"$ref": "definitions.json#/definitions/peopleReferenceList"
						},
						"submitter": {
							"$ref": "definitions.json#/definitions/peopleReference"
						},
						"people": {
							"$ref": "definitions.json#/definitions/peopleReferenceList"
						},
						"projects": {
							"$ref": "definitions.json#/definitions/projectsReferenceList"
						},
						"investigations": {
							"$ref": "definitions.json#/definitions/investigationsReferenceList"
						},
						"studies": {
							"$ref": "definitions.json#/definitions/studiesReferenceList"
						},
						"assays": {
							"$ref": "definitions.json#/definitions/assaysReferenceList"
						},
						"publications": {
							"$ref": "definitions.json#/definitions/publicationsReferenceList"
						},
						"events": {
							"$ref": "definitions.json#/definitions/eventsReferenceList"
						},
						"samples": {
							"$ref": "definitions.json#/definitions/samplesReferenceList"
						}
					},
					"required": [
						"creators",
						"submitter",
						"people",
						"projects",
						"investigations",
						"studies",
						"assays",
						"publications",
						"events",
						"samples"
					]
				},
				"links": {
					"$ref": "definitions.json#/definitions/linksType"
				},
				"meta": {
					"$ref": "definitions.json#/definitions/metaType"
				}
			},
			"required": [
				"id",
				"type",
				"attributes",
				"relationships",
				"links",
				"meta"
			]
		},
		"jsonapi": {
			"$ref": "definitions.json#/definitions/JsonApiVersion"
		}
	},
	"required": [
		"data",
		"jsonapi"
	]
}
