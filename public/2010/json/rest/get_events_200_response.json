					"properties": {
						"title": {
							"$ref": "definitions.json#/definitions/nonEmptyString"
						},
						"description": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"$ref": "definitions.json#/definitions/nonEmptyString"
								}
							]
						},
						"url": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"$ref": "definitions.json#/definitions/uriString"
								}
							]
						},
						"address": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"$ref": "definitions.json#/definitions/nonEmptyString"
								}
							]
						},
						"city": {
							"oneOf": [
								{
									"type": "null"
