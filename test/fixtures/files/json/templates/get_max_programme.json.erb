{
  "data": {
    "id": "<%= res.id %>",
    "type": "programmes",
    "attributes": {
      "avatar": null,
      "title": "A Maximal Programme",
      "description": "A very exciting programme",
      "web_page": "http://www.synbiochem.co.uk",
      "funding_details": "Someone is funding this for me",
      "funding_codes": [
        "DFG"
      ],
      "discussion_links": [
        {
          "id":
          "<%= res.discussion_links.first.id %>",
          "url": "http://www.slack.com/",
          "label": "Slack"
        }
      ]
    },
    "relationships": {
      "administrators": {
        "data": [
          {
            "id": "<%= res.programme_administrators.first.id %>",
            "type": "people"
          }
        ]
      },
      "people": {
        "data":
        <%= res.people.map { |p| { id: p.id.to_s, type: 'people' } }.to_json %>
      },
      "projects": {
        "data": [
          {
            "id": "<%= res.projects.first.id %>",
            "type": "projects"
          }
        ]
      },
      "institutions": {
        "data":
        <%= res.institutions.map { |i| { id: i.id.to_s, type: 'institutions' } }.to_json %>
      },
      "investigations": {
        "data": [
          {
            "id": "<%= res.investigations.first.id %>",
            "type": "investigations"
          }
        ]
      },
      "studies": {
        "data": [
          {
            "id": "<%= res.studies.first.id %>",
            "type": "studies"
          }
        ]
      },
      "assays": {
        "data": [
          {
            "id": "<%= res.assays.first.id %>",
            "type": "assays"
          }
        ]
      },
      "data_files": {
        "data": [
          {
            "id": "<%= res.data_files.first.id %>",
            "type": "data_files"
          }
        ]
      },
      "models": {
        "data": [
          {
            "id": "<%= res.models.first.id %>",
            "type": "models"
          }
        ]
      },
      "sops": {
        "data": [
          {
            "id": "<%= res.sops.first.id %>",
            "type": "sops"
          }
        ]
      },
      "publications": {
        "data": [
          {
            "id": "<%= res.publications.first.id %>",
            "type": "publications"
          }
        ]
      },
      "presentations": {
        "data": [
          {
            "id": "<%= res.presentations.first.id %>",
            "type": "presentations"
          }
        ]
      },
      "events": {
        "data": [
          {
            "id": "<%= res.events.first.id %>",
            "type": "events"
          }
        ]
      },
      "documents": {
        "data": [
          {
            "id": "<%= res.documents.first.id %>",
            "type": "documents"
          }
        ]
      }
    },
    "links": {
      "self": "/programmes/<%= res.id %>"
    },
    "meta": {
      "created": "<%= res.created_at.as_json %>",
      "modified": "<%= res.updated_at.as_json %>",
      "uuid": "<%= res.uuid %>",
      "base_url": "http://localhost:3000",
      "api_version": "0.3"
    }
  },
  "jsonapi": {
    "version": "1.0"
  }
}
